{
	"name": "base-app",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"start": "yarn serve ",
		"bundle-report": "webpack-bundle-analyzer --port 4200 build/stats.json",
		"serve": "cross-env SERVE=true ./node_modules/.bin/webpack serve --env \"REACT_APP_USE_DEV_API=true\" --config webpack.config.js --mode development --open --hot",
		"clean": "rimraf ./build",
		"build-prod": "yarn clean && webpack --mode production --config webpack.config.js --no-devtool",
		"build-dev": "yarn clean && node stamp.js && webpack --env \"REACT_APP_USE_DEV_API=true\" --mode production --config webpack.config.js --no-devtool",
		"server": "node src/server.js",
		"lint": "yarn run lint-es && yarn run lint-styles && yarn run lint-ts && yarn run lint-md",
		"lint-ts": "./node_modules/.bin/tsc --skipLibCheck --incremental --noEmit",
		"lint-es": "./node_modules/.bin/eslint --fix --cache ./**/*.{js,jsx,ts,tsx,json} && prettier --write **/*.{js,jsx,ts,tsx,json,html} --loglevel warn",
		"lint-styles": "./node_modules/.bin/stylelint --fix --cache ./**/*.{css,scss,less} && prettier --write **/*.{css,scss,less} --loglevel warn",
		"lint-md": "./node_modules/.bin/markdownlint --fix **/*.md --ignore node_modules",
		"prepare": "husky install && shx rm -rf .git/hooks && shx ln -s ../.husky .git/hooks && git config --unset core.hooksPath"
	},
	"homepage": "/",
